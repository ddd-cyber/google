{
  "version": 2,
  "buildCommand": "echo 'No build step'",
  "outputDirectory": ".",
  "functions": {
    "api/proxy.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/proxy",
      "dest": "/api/proxy.js"
    }
  ]
}
